#include<bits/stdc++.h>
using namespace std;

void generateSeries(int x) {
    int num = 1;
    for (int i = 0; i < x; i++) {
        cout << num << " ";
        num += 2;
    }
}

int main() {
    int x;

    // get input
    cout << "enter a number: ";
    cin >> x;

    // generating output
    generateSeries(x);

    return 0;
}
